{
  "cli": {
    "version": ">= 16.31.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "http://192.168.20.19:3000",
        "EXPO_PUBLIC_BUILD_ENV": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://backendaliados-sistem.onrender.com",
        "EXPO_PUBLIC_BUILD_ENV": "preview"
      }
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_URL": "https://backendaliados-sistem.onrender.com",
        "EXPO_PUBLIC_BUILD_ENV": "production"
      },
      "secrets": [
        "SUPABASE_URL",
        "SUPABASE_ANON_KEY"
      ]
    }
  },
  "submit": {
    "production": {}
  }
}
